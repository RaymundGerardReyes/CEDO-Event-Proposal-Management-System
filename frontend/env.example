# CEDO Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# Development Environment
NODE_ENV=development

# Backend Configuration
NEXT_PUBLIC_BACKEND_URL=http://localhost:5000
NEXT_PUBLIC_API_URL=http://localhost:5000/api
BACKEND_URL=http://localhost:5000
API_URL=http://localhost:5000/api

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_SECRET_DEV=dev-secret-key-for-development-only
SESSION_TIMEOUT_MINUTES=30

# Google OAuth (Optional)
GOOGLE_CLIENT_ID=your-google-client-id
RECAPTCHA_SITE_KEY=your-recaptcha-site-key

# Development Features
DISABLE_GOOGLE_SIGNIN_IN_DEV=true
ENABLE_DOM_ERROR_RECOVERY=true

# Turbopack Configuration (Optional)
NEXT_TURBOPACK_TRACING=0
