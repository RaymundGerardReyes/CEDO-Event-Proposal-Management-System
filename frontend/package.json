{
    "name": "cedo-partnership-management-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:normal": "next dev",
        "dev:stable": "next dev",
        "dev:turbo": "next dev --turbo || npm run dev:stable",
        "dev:fast": "next dev --turbo || npm run dev:stable",
        "dev:debug": "next dev --turbo || npm run dev:stable",
        "dev:trace": "NEXT_TURBOPACK_TRACING=1 next dev --turbo || npm run dev:stable",
        "dev:fallback": "node scripts/dev-fallback.js",
        "dev:smart": "node scripts/dev-fallback.js",
        "build": "next build",
        "build:analyze": "ANALYZE=true next build",
        "build:profile": "next build --profile",
        "start": "next start",
        "lint": "next lint --cache",
        "lint:fix": "next lint --fix --cache",
        "lint:check": "eslint . --ext .js,.jsx --cache",
        "clean": "rm -rf .next out dist",
        "clean:cache": "rm -rf .next/cache",
        "clean:all": "rm -rf .next out dist node_modules package-lock.json",
        "clean:build": "node scripts/clean-build.js",
        "build:clean": "npm run clean:build && npm run build",
        "reinstall": "npm run clean:all && npm install",
        "profile": "next build --profile",
        "debug": "next dev --turbo || npm run dev:stable",
        "debug:normal": "next dev",
        "test": "vitest",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "test:simple": "vitest --config vitest.test.config.js",
        "test:run": "vitest run --config vitest.test.config.js",
        "test:integration": "node tests/integration/run-integration-tests.js",
        "test:integration:watch": "vitest --config vitest.config.js tests/integration/",
        "test:integration:coverage": "vitest run --coverage --config vitest.config.js tests/integration/",
        "validate:turbopack": "node turbopack-test.js",
        "turbopack:diagnose": "node scripts/turbopack-fix.js",
        "turbopack:fix": "node scripts/turbopack-fix.js --fix",
        "deps:check": "npx npm-check-updates",
        "deps:audit": "npm audit",
        "bundle:analyze": "npx @next/bundle-analyzer",
        "perf:measure": "time npm run build",
        "turbo:check": "next info",
        "turbo:trace": "NEXT_TURBOPACK_TRACING=1 npm run dev",
        "fix:deps": "npm install --force",
        "fix:cache": "npm run clean:cache && npm run dev:stable",
        "fix:turbo": "npm run clean:cache && npm run dev:turbo",
        "test:fonts": "node scripts/test-font-loading.js",
        "fonts:diagnose": "node scripts/test-font-loading.js",
        "test:debug": "vitest --run --reporter=verbose",
        "test:path-safe": "vitest --run --reporter=verbose"
    },
