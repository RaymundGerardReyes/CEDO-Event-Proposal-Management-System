{
    "name": "cedo-partnership-management-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "dev:normal": "next dev",
        "dev:turbo": "next dev --turbo",
        "dev:fast": "next dev --turbo",
        "dev:debug": "cross-env DEBUG=next:* next dev --turbo",
        "dev:trace": "cross-env NEXT_TURBOPACK_TRACING=1 next dev --turbo",
        "build": "next build",
        "build:analyze": "cross-env ANALYZE=true next build",
        "build:profile": "next build --profile",
        "build:production": "cross-env NODE_ENV=production next build",
        "start": "next start",
        "lint": "next lint --cache",
        "lint:fix": "next lint --fix --cache",
        "lint:check": "eslint . --ext .js,.jsx --cache",
        "clean": "rimraf .next out dist",
        "clean:cache": "rimraf .next/cache",
        "clean:all": "rimraf .next out dist node_modules package-lock.json",
        "reinstall": "npm run clean:all && npm install",
        "profile": "next build --profile",
        "debug": "cross-env DEBUG=next:* next dev --turbo",
        "debug:normal": "cross-env DEBUG=next:* next dev",
        "test": "jest --cache",
        "test:watch": "jest --watch --cache",
        "test:config": "node test-config.js",
        "validate:turbopack": "node turbopack-test.js",
        "deps:check": "npx npm-check-updates",
        "deps:audit": "npm audit",
        "bundle:analyze": "npx @next/bundle-analyzer",
        "perf:measure": "time npm run build",
        "test:integration": "jest --config=jest.integration.config.js",
        "turbo:check": "next info",
        "turbo:trace": "cross-env NEXT_TURBOPACK_TRACING=1 npm run dev",
        "test:coverage": "jest --coverage",
        "clear-cache": "rimraf .next && npm run build",
        "clean:force": "node clean-build.js",
        "build:safe": "npm run clean:force && npm run build",
        "analyze:safe": "npm run clean:force && cross-env ANALYZE=true next build"
    },
    "dependencies": {
