{
    "name": "cedo-partnership-management-backend",
    "version": "0.3.0",
    "description": "Backend for Partnership Proposal Management System",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "start:cluster": "node cluster.js",
        "start:prod": "NODE_ENV=production node cluster.js",
        "dev": "nodemon server.js",
        "dev:cluster": "nodemon cluster.js",
        "init-db": "node scripts/init-db.js",
