{
  "name": "cedo-google-auth-monorepo",
  "version": "0.3.0",
  "description": "CEDO Partnership Management System with Google Authentication",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "cd frontend && npm run clean && cd ../backend && rm -rf node_modules",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "lint": "cd frontend && npm run lint",
    "release": "npm version",
    "version:bump": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cedo-google-auth.git"
  },
  "keywords": [
    "cedo",
    "partnership",
    "management",
    "google-auth",
    "oauth",
    "nextjs",
    "express",
    "mysql",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=8.0.0"
  }
}